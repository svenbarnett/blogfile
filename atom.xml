    <summary type="html">&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body&gt;&lt;p&gt;直接上代码&lt;/p&gt;
&lt;pre&gt;&lt;code class=&quot;lang-jsx&quot;&gt;const arr=[&#39;1200.00100&#39;,&#39;1200.00000&#39;,&#39;1200.&#39;,&#39;1200&#39;,&#39;1200.10000&#39;,&#39;0.120010000&#39;,&#39;0.000011111&#39;]
const regexp=/(?:&#92;.0*|(&#92;.&#92;d+?)0+)$/
arr.forEach((item)=&amp;gt;{
    console.log(item.replace(regexp,&#39;$1&#39;))
})

// &amp;gt; 1200.001
// &amp;gt; 1200
// &amp;gt; 1200
// &amp;gt; 1200
// &amp;gt; 1200.1
// &amp;gt; 0.12001
// &amp;gt; 0.000011111
&lt;/code&gt;&lt;/pre&gt;&lt;/body&gt;&lt;/html&gt;</summary>
    
    
    
